<% comment = @review.comment %>
<% rating = @review.rating %>
<% url = @movie_url %>
<div class="show-movie">
  <td><iframe width="700" height="400" src='https://www.youtube.com/embed/<%= url %>?rel=0&autoplay=0' frameborder='0' allowfullscreen></iframe></td>
  <td><div class="star-rating" data-score= <%= rating %>></div></td>
  <td><%= comment %></td>
  <td><%= link_to 'Edit', edit_review_path(@review) %> |
  <%= link_to 'Back', reviews_path %></td>
</div>

<script>
    $('.star-rating').raty({
      path: '/assets/',
      readOnly: true,
      score: function() {
            return $(this).attr('data-score');
    }
  });
</script>
