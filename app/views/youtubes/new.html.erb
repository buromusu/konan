<div class="make-form">
  <%= form_for(@movie) do |f| %>
    <div class="form-group url">
      <%= f.label :url%>
    </div>
    <div class="form-group">
      <%= f.text_area :url %>
    </div>
    <div class="form-group" id="star">
      <%= f.number_field :value, in: 1..5%>
    </div>
    <div class="form-group content">
      <%= f.label :content%>
    </div>
    <div class="form-group">
      <%= f.text_area :content %>
    </div>
    <div class="button">
      <%= f.submit "Create Article", class: "btn btn-primary" %>
    </div>
  <% end %>
</div>
